        <Button x:Name="simbol7" Content="7" HorizontalAlignment="Left" Margin="1060,714,0,0" VerticalAlignment="Top" Height="101" Width="138" Background="#FF509D8D" Foreground="#FFF9E2B9" FontFamily="Arial Black" FontSize="72" Click="simbol7_Click"/>
        <Button x:Name="simbol8" Content="8" HorizontalAlignment="Left" Margin="1256,720,0,0" VerticalAlignment="Top" Height="98" Width="140" Foreground="#FFF9E2B9" Background="#FF509D8D" FontFamily="Arial Black" FontSize="72" Click="simbol8_Click"/>

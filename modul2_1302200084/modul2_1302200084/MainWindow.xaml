        <Button x:Name="simbol0" Content="0" HorizontalAlignment="Left" Margin="1452,343,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.91,0.328" Height="98" Width="140" Foreground="#FFF9E2B9" Background="#FF509D8D" FontFamily="Arial Black" FontSize="72" Click="simbol0_Click"/>

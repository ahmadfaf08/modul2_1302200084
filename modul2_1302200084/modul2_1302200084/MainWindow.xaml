        <Button x:Name="simbol5" Content="5" HorizontalAlignment="Left" Margin="1256,590,0,0" VerticalAlignment="Top" Height="105" Width="140" FontFamily="Arial Black" Foreground="#FFF9E2B9" Background="#FF509D8D" FontSize="72" Click="simbol5_Click"/>

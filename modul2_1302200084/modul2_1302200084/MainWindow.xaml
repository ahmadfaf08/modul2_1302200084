        <Button x:Name="simbolPlus" Content="+" HorizontalAlignment="Left" Margin="1058,343,0,0" VerticalAlignment="Top" Height="98" Width="142" Background="#FF509D8D" Foreground="#FFF9E2B9" FontFamily="Arial Black" FontSize="72" Click="simbolPlus_Click"/>
        <Button x:Name="simbolSamadengan" Content="=" HorizontalAlignment="Left" Margin="1254,343,0,0" VerticalAlignment="Top" Height="98" Width="142" Background="#FF509D8D" Foreground="#FFF9E2B9" FontFamily="Arial Black" FontSize="72" Click="simbolSamadengan_Click"/>
        <TextBox x:Name="labelOutput" HorizontalAlignment="Left" Margin="1064,215,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="524" FontFamily="Arial Black" FontSize="72" TextChanged="labelOutput_TextChanged" Background="#FF509D8D" Foreground="#FFF9E2B9"/>

